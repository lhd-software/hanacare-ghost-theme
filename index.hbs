<script>
  (window.frontendorSliderElement = document.querySelector(
    "#use-unique-id-for-this"
  )),
  (window.FrontendorSlider = {
    element: window.frontendorSliderElement.querySelector(
      ".frontendor-slider"
    ),
    start: function() {
      FrontendorSlider.setActiveSlide(
          FrontendorSlider.element.querySelector(
            ".frontendor-slider__slide--active"
          )
        ),
        FrontendorSlider.setup(),
        FrontendorSlider.element.addEventListener(
          "click",
          FrontendorSlider.handleFrontendorSlidesClick
        );
    },
    handleFrontendorSlidesClick: function(e) {
      if (e.pageX >= window.innerWidth / 2) {
        FrontendorSlider.next();
        var r = FrontendorSlider.element.querySelectorAll(
            ".frontendor-slider__slide"
          ),
          t = [].indexOf.call(
            r,
            window.frontendorSliderElement.querySelector(
              ".frontendor-slider__slide--active"
            )
          );
        FrontendorSlider.fireChangeEvent(t);
      } else {
        FrontendorSlider.previous();
        (r = FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        )),
        (t = [].indexOf.call(
          r,
          window.frontendorSliderElement.querySelector(
            ".frontendor-slider__slide--active"
          )
        ));
        FrontendorSlider.fireChangeEvent(t);
      }
    },
    goTo: function(e) {
      e -= 1;
      var r = FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ),
        t = [].indexOf.call(
          r,
          window.frontendorSliderElement.querySelector(
            ".frontendor-slider__slide--active"
          )
        );
      if (t < e)
        for (let r = t; r < e; r++) FrontendorSlider.next();
      else if (t > e)
        for (let r = t; r > e; r--) FrontendorSlider.previous();
    },
    setup: function() {
      var e = FrontendorSlider.element.querySelectorAll(
        ".frontendor-slider__slide"
      );
      for (let r = 0; r < e.length; r++) {
        const t = e[r];
        0 !== r ?
          FrontendorSlider.setRightSlideCSS(t, r) :
          FrontendorSlider.setActiveSlide(t);
      }
    },
    ajustAllRightSlides: function(e) {
      var r = FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ),
        t = 1;
      for (let n = e; n < r.length; n++) {
        const e = r[n];
        FrontendorSlider.setRightSlideCSS(e, t), t++;
      }
    },
    ajustAllLeftSlides: function(e) {
      var r = FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ),
        t = 1;
      for (let n = e; n >= 0; n--) {
        const e = r[n];
        FrontendorSlider.setLeftSlideCSS(e, t), t++;
      }
    },
    setActiveSlide: function(e) {
      e.style.setProperty("--transform", "scale(1) translateX(0px)"),
        e.style.setProperty("--opacity", 1),
        e.style.setProperty("--z-index", 20),
        e.style.setProperty("--filter", "blur(0px)");
    },
    setLeftSlideCSS: function(e, r) {
      var t = window
        .getComputedStyle(FrontendorSlider.element)
        .getPropertyValue("--image-space"),
        n =
        (frontendorSliderElement &&
          parseFloat(
            window.frontendorSliderElement.getAttribute(
              "data-scale-decrease"
            )
          )) ||
        0.07,
        o =
        (frontendorSliderElement &&
          parseFloat(
            window.frontendorSliderElement.getAttribute(
              "data-opacity-decrease"
            )
          )) ||
        0,
        l =
        (frontendorSliderElement &&
          parseFloat(
            window.frontendorSliderElement.getAttribute(
              "data-blur-increase"
            )
          )) ||
        7,
        i = r,
        d = 1 - n * i,
        s = t * i,
        a =
        FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ).length - i,
        u = 1 - o * i,
        c = l * i;
      e.setAttribute("data-translate-x", s),
        e.style.setProperty(
          "--transform",
          "scale(" + d + ") translateX(-" + s + "px)"
        ),
        e.style.setProperty("--z-index", a),
        e.style.setProperty("--opacity", u),
        e.style.setProperty("--filter", "blur(" + c + "px)");
    },
    setRightSlideCSS: function(e, r) {
      var t = window
        .getComputedStyle(FrontendorSlider.element)
        .getPropertyValue("--image-space"),
        n =
        (frontendorSliderElement &&
          parseFloat(
            window.frontendorSliderElement.getAttribute(
              "data-scale-decrease"
            )
          )) ||
        0.07,
        o =
        (frontendorSliderElement &&
          parseFloat(
            window.frontendorSliderElement.getAttribute(
              "data-opacity-decrease"
            )
          )) ||
        0,
        l =
        (frontendorSliderElement &&
          parseFloat(
            window.frontendorSliderElement.getAttribute(
              "data-blur-increase"
            )
          )) ||
        7,
        i = r,
        d = 1 - n * i,
        s = t * i,
        a =
        FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ).length - i,
        u = 1 - o * i,
        c = l * i;
      e.setAttribute("data-translate-x", s),
        e.style.setProperty(
          "--transform",
          "scale(" + d + ") translateX(" + s + "px)"
        ),
        e.style.setProperty("--z-index", a),
        e.style.setProperty("--opacity", u),
        e.style.setProperty("--filter", "blur(" + c + "px)");
    },
    handleSliderNotActiveAnimation: function(e) {
      e.target.classList.remove("frontendor-slider__slide--not-active"),
        e.target.removeEventListener(
          "animationend",
          FrontendorSlider.handleSliderNotActiveAnimation
        );
    },
    next: function() {
      var e = FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ),
        r = FrontendorSlider.element.querySelector(
          ".frontendor-slider__slide--active"
        ),
        t = [].indexOf.call(e, r);
      if (t >= e.length - 1) return FrontendorSlider.reset();
      r.addEventListener(
          "animationend",
          FrontendorSlider.handleSliderNotActiveAnimation
        ),
        r.classList.replace(
          "frontendor-slider__slide--active",
          "frontendor-slider__slide--not-active"
        ),
        e[++t].classList.add("frontendor-slider__slide--active"),
        FrontendorSlider.setActiveSlide(e[t]),
        FrontendorSlider.ajustAllRightSlides(t + 1),
        FrontendorSlider.ajustAllLeftSlides(t - 1);
    },
    previous: function() {
      var e = FrontendorSlider.element.querySelectorAll(
          ".frontendor-slider__slide"
        ),
        r = FrontendorSlider.element.querySelector(
          ".frontendor-slider__slide--active"
        ),
        t = [].indexOf.call(e, r);
      0 !== t &&
        (r.addEventListener(
            "animationend",
            FrontendorSlider.handleSliderNotActiveAnimation
          ),
          r.classList.replace(
            "frontendor-slider__slide--active",
            "frontendor-slider__slide--not-active"
          ),
          e[--t].classList.add("frontendor-slider__slide--active"),
          FrontendorSlider.setActiveSlide(e[t]),
          FrontendorSlider.ajustAllLeftSlides(t - 1),
          FrontendorSlider.ajustAllRightSlides(t + 1));
    },
    reset: function() {
      var e = FrontendorSlider.element.querySelectorAll(
        ".frontendor-slider__slide"
      );
      for (let r = 0; r < e.length; r++) FrontendorSlider.previous();
    },
    fireChangeEvent(e) {
      var r = new CustomEvent("frontendor-slide-change", {
        detail: {
          slideNumber: e + 1
        },
      });
      window.dispatchEvent(r);
    },
  }),
  (window.FrontendorMenu = {
    element: window.frontendorSliderElement.querySelector(".slider-menu"),
    currentIntervalTime: null,
    defaultIntervalTime: (frontendorSliderElement &&
        parseFloat(
          window.frontendorSliderElement.getAttribute(
            "data-default-slider-timeout"
          )
        )) ||
      3e3,
    intervalTimeForLargeScreen: (frontendorSliderElement &&
      JSON.parse(
        window.frontendorSliderElement.getAttribute(
          "data-large-screen-slider-timeout"
        )
      )) || [],
    intervalTimeForSmallScreen: (frontendorSliderElement &&
      JSON.parse(
        window.frontendorSliderElement.getAttribute(
          "data-large-screen-slider-timeout"
        )
      )) || [],
    timeout: null,
    intervalTime: [],
    start: function() {
      window.matchMedia("(min-width: 768px)").matches ?
        (options = FrontendorMenu.intervalTimeForLargeScreen) :
        (options = FrontendorMenu.intervalTimeForSmallScreen),
        (FrontendorMenu.intervalTime = options),
        FrontendorMenu.element.addEventListener(
          "click",
          FrontendorMenu.handleMenuClick
        ),
        FrontendorMenu.element
        .querySelectorAll(".slider-menu__item")[0]
        .classList.add("slider-menu__item--active"),
        (FrontendorMenu.currentIntervalTime =
          FrontendorMenu.intervalTime[0] ||
          FrontendorMenu.defaultIntervalTime),
        FrontendorMenu.element.style.setProperty(
          "--slider-menu-item-transition",
          FrontendorMenu.currentIntervalTime + "ms"
        ),
        (FrontendorMenu.timeout = setTimeout(function() {
          window.FrontendorSlider.goTo(2), FrontendorMenu.gotTo(2);
        }, FrontendorMenu.currentIntervalTime));
    },
    handleMenuClick: function(e) {
      var r = e.target.closest(".slider-menu__item");
      FrontendorMenu.gotTo(r);
    },
    gotTo(e) {
      var r = FrontendorMenu.element.querySelectorAll(".slider-menu__item");
      "number" == typeof e && (e = r[e - 1]),
        FrontendorMenu.timeout && clearTimeout(FrontendorMenu.timeout);
      var t = null;
      if (e) {
        FrontendorMenu.element
          .querySelectorAll(".slider-menu__item")
          .forEach(function(e) {
            e.classList.remove("slider-menu__item--active");
          }),
          e.classList.add("slider-menu__item--active");
        var n = [].indexOf.call(r, e);
        window.FrontendorSlider.goTo(n + 1), (t = n + 1);
      } else {
        var o = FrontendorMenu.element.querySelector(
            ".slider-menu__item--active"
          ),
          l = [].indexOf.call(r, o),
          i = r[l + 1];
        (t = l + 1),
        i || ((i = r[0]), (t = 0)),
          FrontendorMenu.element
          .querySelectorAll(".slider-menu__item")
          .forEach(function(e) {
            e.classList.remove("slider-menu__item--active");
          }),
          i.classList.add("slider-menu__item--active");
      }
      (FrontendorMenu.currentIntervalTime =
        FrontendorMenu.intervalTime[t - 1] ||
        FrontendorMenu.intervalTime[0] ||
        FrontendorMenu.defaultIntervalTime),
      FrontendorMenu.element.style.setProperty(
          "--slider-menu-item-transition",
          FrontendorMenu.currentIntervalTime + "ms"
        ),
        (FrontendorMenu.timeout = setTimeout(function() {
          window.FrontendorSlider.goTo(t + 1), FrontendorMenu.gotTo(t + 1);
        }, FrontendorMenu.currentIntervalTime));
    },
  }),
  window.addEventListener("frontendor-slide-change", function(e) {
      FrontendorMenu.element
        .querySelectorAll(".slider-menu__item")[e.detail.slideNumber - 1].click();
    }),
    FrontendorSlider.start(),
    FrontendorMenu.start();
</script>


